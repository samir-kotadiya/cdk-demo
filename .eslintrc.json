{
  "extends": "./node_modules/gts/",
  "rules": {
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/explicit-function-return-type": "warn",
    "no-restricted-imports": "warn"
  },
  "overrides": [
    {
      "files": [
        "src/**/*.ts"
      ],
      "rules": {
        "no-restricted-imports": [
          "error",
          {
            "patterns": [
              ".*"
            ]
          }
        ]
      }
    }
  ]
}
